console.log('Taskhome Callback');

function jumlah(a, b, hasil){
    setTimeout(()=> {
        hasil(a + b);
    });
 }
 let hasilPenjumlahan = 0;
 jumlah(10, 20, (result)=> {
    console.log(result);
 })

function kurang(a, b, hasil){
    setTimeout(()=> {
        hasil(a - b);
    });
 }
 let hasilPengurangan = 0;
 kurang(10, 20, (result)=> {
    console.log(result);
 })

